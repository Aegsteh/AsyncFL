{
    "client": {
        "batch_size": 64,
        "local epoch": 15,
        "optimizer": {
            "method": "SGD",
            "lr": 0.01,
            "momentum": 0.9
        }
    },
    "global":{
        "model": "VGG11s_3",
        "dataset": "CIFAR10",
        "loss function": "CrossEntropy",
        "n_clients": 5,
        "mode": "FedBuff",
        "updater":{
            "method": "K_number",
            "params":{
                "K": 5
            }
        },
        "epoch": 500,
        "schedule_period": 2,
        "schedule":{
            "method" : "idle",
            "params":{
                "proportion" : 0.8
            }
        },
        "delay" :{
            "mode" : "base",
            "param": 5
        }
    },
    "device": "gpu",
    "data_distribution": {
        "iid" : false,
        "customize": false,
        "cus_distribution": [2,2,2,2,2,2,2,2,2,2]
    },
    "compressor":{
        "uplink":{
            "method":"topk",
            "params":{
                "cr": 0.1,
                "error_feedback":false
            }
        },
        "downlink": {
            "method": "none",
            "params": {
                "cr": 1,
                "error_feedback": true
            }
        }
    },
    "result":{
        "path": "FedBuff/CR_ACC_NIID_NE"
    }
}