{
    "client": {
        "batch_size": 64,
        "local epoch": 1,
        "optimizer": {
            "method": "SGD",
            "lr": 0.001,
            "momentum": 0.9
        }
    },
    "global":{
        "model": "VGG11s",
        "dataset": "FMNIST",
        "loss function": "CrossEntropy",
        "n_clients": 5,
        "mode": "async",
        "updater":{
            "method": "period",
            "params":{
                "alpha": 1
            }
        },
        "epoch": 100,
        "epoch_time": 8,
        "schedule":{
            "method" : "idle",
            "params":{
                "proportion" : 0.8
            }
        },
        "delay" :{
            "mode" : "base",
            "param": 5
        }
    },
    "device": "gpu",
    "data_distribution": {
        "iid" : true,
        "customize": false,
        "cus_distribution": [2,2,2,2,2,2,2,2,2,2]
    },
    "compressor":{
        "uplink":{
            "method":"topk",
            "params":{
                "cr": 0.1,
                "error_feedback":true
            }
        },
        "downlink": {
            "method": "None",
            "params": {
                "cr": 0.01,
                "error_feedback": true
            }
        }
    },
    "result":{
        "path": "CRandStaleness"
    }
}