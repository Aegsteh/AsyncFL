{
    "client": {
        "loss function": "CrossEntropy",
        "batch_size": 200,
        "local epoch": 1,
        "optimizer": {
            "method": "SGD",
            "lr": 0.0001,
            "momentum": 0.9
        }
    },
    "global":{
        "model": "CNN1",
        "dataset": "MNIST",
        "mode": "async",
        "updater":{
            "method": "single",
            "params":{
                "alpha": 1
            }
        },
        "epoch": 100,
        "n_clients" : 1,
        "schedule":{
            "method" : "random",
            "params":{
                "proportion" : 0.8
            }
        }
    },
    "device": "gpu",
    "data_distribution": {
        "iid" : true,
        "customize": false,
        "cus_distribution": [2,2,2,2,2,2,2,2,2,2]
    },
    "compressor":{
        "uplink":{
            "method":"None",
            "params":{
                "cr": 0.01,
                "error_feedback":true
            }
        },
        "downlink": {
            "method": "None",
            "params": {
                "cr": 0.01,
                "error_feedback": true
            }
        }
    }
}