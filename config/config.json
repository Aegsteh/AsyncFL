{
    "client": {
        "dataset": "MNIST",
        "loss function": "CrossEntropy",
        "batch_size": 8,
        "local epoch": 60,
        "optimizer": {
            "method": "SGD",
            "lr": 0.01,
            "momentum": 0.9
        }
    },
    "global":{
        "model": "CNN1",
        "mode": "async",
        "n_clients" : 10
    },
    "device": "gpu",
    "data_distribution": {
        "iid" : false,
        "customize": false,
        "cus_distribution": [2,2,2,2,2,2,2,2,2,2]
    },
    "compressor":{
        "uplink":{
            "method":"topk",
            "params":{
                "cr": 0.01,
                "error_feedback":true
            }
        },
        "downlink": {
            "method": "None",
            "params": {
                "cr": 0.01,
                "error_feedback": true
            }
        }
    }
}