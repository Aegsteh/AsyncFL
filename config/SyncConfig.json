{
    "client": {
        "batch_size": 64,
        "local epoch": 15,
        "optimizer": {
            "method": "SGD",
            "lr": 0.01,
            "momentum": 0.9
        }
    },
    "global": {
        "model": "CNN1",
        "dataset": "FMNIST",
        "loss function": "CrossEntropy",
        "n_clients": 3,
        "mode": "async",
        "updater": {
            "method": "period",
            "params": {
                "alpha": 1
            }
        },
        "epoch": 300,
        "schedule": {
            "method": "idle",
            "params": {
                "proportion": 0.8
            }
        },
        "delay": {
            "mode": "base",
            "param": 4
        }
    },
    "device": "gpu",
    "data_distribution": {
        "iid": false,
        "customize": false,
        "cus_distribution": [
            2,
            2,
            2,
            2,
            2,
            2,
            2,
            2,
            2,
            2
        ]
    },
    "compressor": {
        "uplink": {
            "method": "topk",
            "params": {
                "cr": 1,
                "error_feedback": false
            }
        },
        "downlink": {
            "method": "none",
            "params": {
                "cr": 1,
                "error_feedback": true
            }
        }
    },
    "result": {
        "path": "Sync/Test"
    }
}